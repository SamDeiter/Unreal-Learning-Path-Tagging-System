{
  "name": "unreal-learning-path-tagging-system",
  "version": "2.8.0",
  "description": "UE5 Learning Path Builder with AI-enriched course tagging",
  "scripts": {
    "start": "npx -y http-server -p 8080 --cors -o",
    "scan": "node scripts/scan_video_library.js",
    "analyze": "node scripts/analyze_transcripts_ai.js",
    "update": "node scripts/update_and_deploy.js",
    "lint": "eslint scripts/**/*.js",
    "lint:fix": "eslint scripts/**/*.js --fix",
    "format": "prettier --write \"**/*.{js,json,html,css}\"",
    "test": "node --test tests/*.test.js"
  },
  "devDependencies": {
    "dotenv": "^17.2.3",
    "eslint": "^9.0.0",
    "prettier": "^3.2.0"
  },
  "dependencies": {
    "@google/generative-ai": "^0.24.1"
  }
}
