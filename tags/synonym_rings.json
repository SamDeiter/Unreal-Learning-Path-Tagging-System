{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "synonym_rings.schema.json",
  "title": "UE5 Synonym Ring Database",
  "version": "1.0.0",
  "description": "Maps user search terms to canonical tag IDs. When a user searches for 'BP' or 'Visual Scripting', the system resolves to the canonical 'scripting.blueprint' tag. This improves search discoverability across different terminology users might know.",

  "_design_notes": {
    "purpose": "Bridge the gap between how developers describe problems and how we categorize solutions",
    "usage": "Query normalization layer - run user input through synonym lookup before tag matching",
    "maintenance": "Add new synonyms when users consistently fail to find content using common terms"
  },

  "synonym_rings": [
    {
      "_comment": "Blueprint is the #1 searched term but users say 'BP' or 'visual scripting' interchangeably",
      "canonical_tag": "scripting.blueprint",
      "display_name": "Blueprint",
      "synonyms": [
        "BP",
        "Visual Scripting",
        "Node Graph",
        "Event Graph",
        "Blueprint Visual Scripting"
      ],
      "scope": "Scripting Logic"
    },
    {
      "_comment": "Packaging has 7+ synonyms because the workflow spans build/cook/package/deploy",
      "canonical_tag": "build.packaging",
      "display_name": "Packaging",
      "synonyms": [
        "Build",
        "Cook",
        "Export",
        "Make EXE",
        "Deploy",
        "Ship",
        "Distribute"
      ],
      "scope": "Deployment"
    },
    {
      "_comment": "Lumen is UE5's GI system - many users don't know the name, just 'global illumination'",
      "canonical_tag": "rendering.lumen",
      "display_name": "Lumen",
      "synonyms": [
        "Global Illumination",
        "GI",
        "Dynamic Lighting",
        "Ray Traced GI",
        "Software RT"
      ],
      "scope": "Rendering"
    },
    {
      "_comment": "ExitCode=25 is the infamous 'Unknown Cook Failure' - users paste this from logs constantly",
      "canonical_tag": "build.exitcode_25",
      "display_name": "ExitCode=25",
      "synonyms": [
        "Unknown Cook Failure",
        "AutomationTool Error",
        "UAT Fail",
        "Error_UnknownCookFailure"
      ],
      "scope": "Error Codes"
    },
    {
      "_comment": "UMG is the formal name but users say 'UI' or 'Widget' far more often",
      "canonical_tag": "ui.umg",
      "display_name": "UMG",
      "synonyms": [
        "UI",
        "Widget",
        "HUD",
        "Interface",
        "User Interface",
        "Widget Blueprint"
      ],
      "scope": "User Interface"
    },
    {
      "_comment": "C++ in UE has many nicknames including 'native code' and just 'code'",
      "canonical_tag": "scripting.cpp",
      "display_name": "C++",
      "synonyms": [
        "CPP",
        "Native",
        "Code",
        "VS",
        "Visual Studio",
        "Native Code"
      ],
      "scope": "Programming"
    },
    {
      "_comment": "Nanite is UE5's geometry system - 'virtual geometry' is the technical term",
      "canonical_tag": "rendering.nanite",
      "display_name": "Nanite",
      "synonyms": ["Virtualized Geometry", "Micropolygon", "Virtual Geometry"],
      "scope": "Rendering"
    },
    {
      "_comment": "Niagara replaced Cascade - many UE4 users still search for 'particles' or 'Cascade'",
      "canonical_tag": "rendering.niagara",
      "display_name": "Niagara",
      "synonyms": ["VFX", "Particles", "Effects", "Particle System", "Cascade"],
      "scope": "Visual Effects"
    },
    {
      "_comment": "GPU crashes appear with many names - TDR, device lost, device removed",
      "canonical_tag": "crash.d3d_device_lost",
      "display_name": "D3D Device Lost",
      "synonyms": [
        "GPU Crash",
        "DXGI_ERROR_DEVICE_REMOVED",
        "TDR",
        "Timeout Detection",
        "Graphics Crash"
      ],
      "scope": "Crashes"
    },
    {
      "_comment": "Control Rig is the new animation system - related to IK and procedural animation",
      "canonical_tag": "animation.control_rig",
      "display_name": "Control Rig",
      "synonyms": ["IK Rig", "Rigging", "Animation Rig", "Full Body IK"],
      "scope": "Animation"
    },
    {
      "_comment": "MetaSounds replaced Sound Cues for procedural audio",
      "canonical_tag": "audio.metasounds",
      "display_name": "MetaSounds",
      "synonyms": [
        "Procedural Audio",
        "Audio Graph",
        "Sound Graph",
        "MetaSound"
      ],
      "scope": "Audio"
    },
    {
      "_comment": "Networking/Replication is searched as 'multiplayer', 'netcode', or 'RPC'",
      "canonical_tag": "multiplayer.replication",
      "display_name": "Replication",
      "synonyms": [
        "Networking",
        "Multiplayer",
        "Net Code",
        "RPC",
        "RepNotify",
        "Server Auth"
      ],
      "scope": "Networking"
    },
    {
      "_comment": "Meta Quest has gone through several brand names - Oculus, Quest, Meta",
      "canonical_tag": "platform.quest",
      "display_name": "Meta Quest",
      "synonyms": ["Oculus Quest", "Quest 2", "Quest 3", "Quest Pro", "Oculus"],
      "scope": "VR/XR"
    },
    {
      "_comment": "Fab is the new unified marketplace - users still search for 'Marketplace' and 'Megascans'",
      "canonical_tag": "asset.fab",
      "display_name": "Fab",
      "synonyms": [
        "Marketplace",
        "UE Marketplace",
        "Asset Store",
        "Quixel Bridge",
        "Megascans"
      ],
      "scope": "Asset Management"
    },
    {
      "_comment": "Accessed None is THE most common Blueprint runtime error - many names for it",
      "canonical_tag": "blueprint.accessed_none",
      "display_name": "Accessed None",
      "synonyms": [
        "Null Reference",
        "Is Valid",
        "None Error",
        "Null Pointer",
        "NullRef"
      ],
      "scope": "Blueprint Errors"
    },
    {
      "_comment": "Material is the #1 most-tagged topic (50 courses) — users say 'shader' or 'texture' interchangeably",
      "canonical_tag": "rendering.material",
      "display_name": "Material",
      "synonyms": ["Shader", "Texture", "Material Editor", "Material Graph", "Surface", "PBR"],
      "scope": "Rendering"
    },
    {
      "_comment": "Sequencer is used in 43 courses — users search 'cinematic' or 'cutscene' or 'camera animation'",
      "canonical_tag": "cinematic.sequencer",
      "display_name": "Sequencer",
      "synonyms": ["Cinematic", "Cutscene", "Camera Animation", "Movie Render", "Level Sequence"],
      "scope": "Cinematics"
    },
    {
      "_comment": "Animation covers 38 courses — broad category from skeletal mesh to anim BP",
      "canonical_tag": "animation.general",
      "display_name": "Animation",
      "synonyms": ["Skeletal Mesh", "Anim Blueprint", "Montage", "Animation Sequence", "Retargeting", "Motion"],
      "scope": "Animation"
    },
    {
      "_comment": "Lighting spans 34 courses — users say 'lights' or 'illumination' or 'shadows'",
      "canonical_tag": "rendering.lighting",
      "display_name": "Lighting",
      "synonyms": ["Lights", "Illumination", "Shadows", "Light Actor", "Sky Light", "Point Light", "Spot Light"],
      "scope": "Rendering"
    },
    {
      "_comment": "Landscape covers 18 courses — terrain, heightmap, world partition",
      "canonical_tag": "environment.landscape",
      "display_name": "Landscape",
      "synonyms": ["Terrain", "Heightmap", "Landscape Material", "World Partition", "Open World"],
      "scope": "Environment"
    },
    {
      "_comment": "Foliage covers 15 courses — grass, vegetation, trees",
      "canonical_tag": "environment.foliage",
      "display_name": "Foliage",
      "synonyms": ["Vegetation", "Trees", "Grass", "Plants", "Procedural Foliage", "Foliage Tool"],
      "scope": "Environment"
    },
    {
      "_comment": "LOD Optimization covers 15 courses — level of detail, mesh reduction",
      "canonical_tag": "optimization.lod",
      "display_name": "LOD",
      "synonyms": ["Level of Detail", "Mesh Reduction", "Auto LOD", "HLOD", "LOD Group"],
      "scope": "Optimization"
    },
    {
      "_comment": "MetaHuman covers 9 courses — digital humans, face animation",
      "canonical_tag": "character.metahuman",
      "display_name": "MetaHuman",
      "synonyms": ["Digital Human", "MetaHuman Creator", "Face Animation", "Facial Rig", "DNA Calibration"],
      "scope": "Character"
    },
    {
      "_comment": "Ray Tracing covers 8 courses — hardware RT, path tracing",
      "canonical_tag": "rendering.raytracing",
      "display_name": "Ray Tracing",
      "synonyms": ["Hardware RT", "Path Tracing", "RTX", "DXR", "Ray Traced Shadows"],
      "scope": "Rendering"
    },
    {
      "_comment": "Python scripting covers 8 courses — automation, editor scripting",
      "canonical_tag": "scripting.python",
      "display_name": "Python",
      "synonyms": ["Python Scripting", "Editor Scripting", "Automation", "Python API"],
      "scope": "Scripting"
    },
    {
      "_comment": "Post Processing covers 11 courses — color grading, bloom, DOF",
      "canonical_tag": "rendering.postprocess",
      "display_name": "Post Processing",
      "synonyms": ["Post Process", "Color Grading", "Bloom", "Depth of Field", "DOF", "Tone Mapping"],
      "scope": "Rendering"
    },
    {
      "_comment": "AI Navigation covers 6 courses — nav mesh, behavior tree, pathfinding",
      "canonical_tag": "ai.navigation",
      "display_name": "AI Navigation",
      "synonyms": ["Nav Mesh", "Pathfinding", "Behavior Tree", "EQS", "AI Controller", "NPC"],
      "scope": "AI"
    }
  ],

  "fuzzy_matching": {
    "_comment": "Rules to handle compound word and spacing issues common in search",
    "description": "Normalizes user input before synonym lookup - handles typos and formatting variations",
    "examples": [
      {
        "input": "swimmingpool",
        "normalized": "swimming pool",
        "_note": "Compound words are often written without spaces"
      },
      {
        "input": "snow mobile",
        "normalized": "snowmobile",
        "_note": "Or with spaces where there shouldn't be"
      },
      {
        "input": "exitcode25",
        "normalized": "ExitCode=25",
        "_note": "Error codes need exact formatting for lookup"
      }
    ]
  }
}
